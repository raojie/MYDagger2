<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raoj.dagger">

    <application
        android:name=".presentation.AndroidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.view.activity.UserListActivity"
            android:label="@string/activity_title_user_list">
        </activity>

        <activity
            android:name=".presentation.view.activity.UserDetailsActivity"
            android:label="@string/activity_title_user_details">
        </activity>
    </application>

</manifest>